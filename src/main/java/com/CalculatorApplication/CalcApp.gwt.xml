<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.0//EN"
        "http://gwtproject.org/doctype/2.8.2/gwt-module.dtd">
<module rename-to="CalcApp">
    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>

    <!-- Specify the app entry point class.                   -->
    <entry-point class='com.CalculatorApplication.client.CalcApp'/>

    <!-- Specify the app servlets.                   -->
    <servlet path='/CalcAppService' class='com.CalculatorApplication.server.CalcAppServiceImpl'/>
</module>